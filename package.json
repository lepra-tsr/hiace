    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "firebase": "^10.6.0",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "svelte": "^4.0.3",
    "svelte-check": "^3.6.0",
    "svelte-routing": "^2.6.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
