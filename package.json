{
  "name": "hiace",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "npm run check && npm run lint && npm run prettier",
    "prettier": "npx prettier . --write",
    "lint": "npx eslint . --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings a11y-no-static-element-interactions:ignore,a11y-mouse-events-have-key-events:ignore,a11y-click-events-have-key-events:ignore",
    "check_watch": "npm run check -- --watch",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.2",
    "firebase": "^10.0.0",
    "prettier": "3.0.0",
    "svelte": "^4.0.3",
    "svelte-check": "^3.4.4",
    "svelte-routing": "^1.11.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  }
}
